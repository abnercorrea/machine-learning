E"},"source":["# Util"]},{"cell_type":"code","metadata":{"id":"w9MUUv9urZwj"},"source":["def quick_sort(a):\n","  def qsort(s, e):\n","    \"\"\"\n","    two-way partitioning.\n","    \"\"\"\n","    if s >= e:\n","      return \n","    i = s\n","    j = e + 1\n","    pivot = a[s]\n","    while True:\n","      i +=1\n","      j -= 1\n","      while i <= e and a[i] < pivot:\n","        i += 1\n","      while a[j] > pivot:\n","        j -= 1         \n","      if i > j:\n","        break\n","      a[i], a[j] = a[j], a[i]  # swap i,j\n","    a[s], a[j] = a[j], a[s]  # swap s, j\n","    \n","    qsort(s, j - 1)\n","    qsort(j + 1, e)\n","  \n","  qsort(0, len(a) - 1)"],"execution_count":null,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"-CZD5wuufhJR"},"source":["# Numpy"]},{"cell_type":"code","metadata":{"id":"y67jS0mrAtTI"},"source":["x = np.random.rand(2) * 100"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"_rVXWHCefaLP"},"source":["x"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"8QpIuicaBVTR"},"source":["x**2"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"YBnaBPNDEeTN"},"source":["np.sum(x**2)"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"8yrvJzYGEiI4"},"source":["np.sqrt(np.sum(x**2))"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"TrrrM6nMEsw4"},"source":["%timeit x / np.sqrt(np.sum(x**2))"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"paUA4AnmegI9"},"source":["%timeit preprocessing.normalize(x[:,np.newaxis], axis=0).ravel()"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"6jE0NIRVevac"},"source":["np.all(x / np.sqrt(np.sum(x**2)) == preprocessing.normalize(x[:,np.newaxis], axis=0).ravel())"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"PC3oWla4E05r"},"source":["sp.linalg.norm(x / np.sqrt(np.sum(x**2)))"],"execution_coun